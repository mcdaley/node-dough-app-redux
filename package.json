{
  "name": "dough-app-redux",
  "version": "1.0.0",
  "description": "Node personal finance app w/ redux store",
  "main": "index.js",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "local-storage": "^2.0.0",
    "mongoose": "^5.9.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "app-root-path": "^3.0.0",
    "chai": "^4.2.0",
    "concurrently": "^5.1.0",
    "dotenv": "^8.2.0",
    "mocha": "^7.1.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2",
    "winston": "^3.2.1"
  },
  "scripts": {
    "client": "npm run start --prefix client",
    "server": "export NODE_ENV=development; nodemon index.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start": "node index.js",
    "test": "export NODE_ENV=test; mocha --exit server/**/*.test.js",
    "single-test": "export NODE_ENV=test; mocha --exit"
  },
